@prefix dcterms: <http://purl.org/dc/terms/> .
@prefix ex: <http://example.com/ns#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

ex:NodeShape
    a sh:NodeShape ;
    sh:targetClass ex:BookClass ;
    sh:property [
        sh:path dcterms:title ; 
        sh:minCount 1 ;
        sh:maxCount 2 ;
    ] .
